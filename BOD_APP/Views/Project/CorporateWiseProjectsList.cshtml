@model IList<BOD_DAL.Models.ProjectEntry>
@{
    ViewBag.Title = "CorporateWiseProjectsList";
    Layout = "~/Views/Shared/_LayoutNewTempDatatable.cshtml";
    }


<div class="page-title">
    <div class="row justify-content-between align-items-center">
        <div class="col-md-6 mb-3 mb-md-0">
            <h5 class="h3 font-weight-400 mb-0 text-white">Enterprise Client Wise Project</h5>
           
        </div>
    </div>
</div>
<div class="row">
    <div class="col-xl-12 col-md-6">
        <div class="card card-fluid">
            <div class="card-header">
                <h6 class="mb-0">Enterprise Client Wise Project List</h6>
            </div>
            <div class="card-body">
                <!-- Chart -->
                <div class="table-responsive">
                    <table id="invoices-list" class="table table-white-space table-bordered row-grouping display no-wrap icheck table-middle">
                        <thead>
                            <tr>
                                <th><input type="checkbox" class="input-chk-all"></th>
                                <th>Project</th>
                                <th>Enterprise Name</th>
                                <th>From -To </th>
                                <th>Details</th>

                            </tr>
                        </thead>
                        <tbody>


                            @{ int i = 1; foreach (var item in Model)
                             {

                                <tr>
                                    <td><input type="checkbox" class="input-chk"></td>
                                    <td><a href="ProjectSummary?Id=@item.PE_Id" class=" text-bold-600">@item.PE_ProjectName </a></td>

                                    <td>
                                        <span class="badge badge-success badge-md"> @item.CC_CorporateName.Trim()</span>

                                    </td>
                                    <td><p class="text-muted text-bold-500">@item.PE_StartDate  to  @item.PE_EndDate</p></td>


                                    <td><p class="text-muted text-bold-500" style="white-space:nowrap;text-overflow: ellipsis;width:300px; overflow: hidden;">@item.PE_Description</p></td>





                                </tr>
                             }
                            }
                        </tbody>

                    </table>
                </div>

            </div>
        </div>
    </div>

</div>

<script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.7/umd/popper.min.js" integrity="sha384-UO2eT0CpHqdSJQ6hJty5KVphtPhzWj9WO1clHTMGa3JDZwrnQq4sF86dIHNDz0W1" crossorigin="anonymous"></script>
<script src="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/js/bootstrap.min.js" integrity="sha384-JjSmVgyd0p3pXB1rRibZUAYoIIy6OrQ6VrjIEaFf/nJGzIxFDsf4x0xIM+B07jRM" crossorigin="anonymous"></script>


