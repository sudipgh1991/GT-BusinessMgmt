                                <tbody id="tbodyid">
                                    <tr>

                                        <td><input type="text" class="form-control" value="SSC/O-Level" id="txtExam_1" /></td>
                                        <td><input type="text" class="form-control" id="txtGrpSub_1" /></td>
                                        <td><input type="text" class="form-control" id="txtPassingYr_1" /></td>
                                        <td><input type="text" class="form-control" id="txtGpa_1" /></td>

                                    </tr>
                                    <tr>

                                        <td><input type="text" class="form-control" value="HSC/A-Level" id="txtExam_2" /></td>
                                        <td><input type="text" class="form-control" id="txtGrpSub_2" /></td>
                                        <td><input type="text" class="form-control" id="txtPassingYr_2" /></td>
                                        <td><input type="text" class="form-control" id="txtGpa_2" /></td>

