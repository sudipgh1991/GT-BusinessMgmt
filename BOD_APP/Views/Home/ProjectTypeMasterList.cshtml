@model IList<BOD_DAL.Models.ProjectTypeMaster>
@{
    ViewBag.Title = "ProjectTypeMasterList";
    Layout = "~/Views/Shared/_MainLayout.cshtml";
}


          
          
@Styles.Render("~/datatable/css")
         <!-- HTML5 export buttons table -->
            <section id="html5">
                <div class="row">
                    <div class="col-12">
                        <div class="card">
                            <div class="card-header">
                                <h4 class="card-title">Project Type List</h4>
                                <a class="heading-elements-toggle"><i class="la la-ellipsis-v font-medium-3"></i></a>
                                <div class="heading-elements">
                                    <button id="btnGo" class="btn btn-primary btn-sm"><i class="ft-plus white"></i> Add Type</button>
                                    @*<ul class="list-inline mb-0">
                                        <li><a data-action="collapse"><i class="ft-minus"></i></a></li>
                                        <li><a data-action="reload"><i class="ft-rotate-cw"></i></a></li>
                                        <li><a data-action="expand"><i class="ft-maximize"></i></a></li>
                                        <li><a data-action="close"><i class="ft-x"></i></a></li>
                                    </ul>*@
                                </div>
                            </div>
                            <div class="card-content collapse show">
                                <div class="card-body card-dashboard">

                                    <div class="table-responsive">
                                        <table class="table table-striped table-bordered dataex-html5-export ">
                                            <thead>
                                                <tr>
                                                    <th width="90%">Types</th>
                                                    <th>Edit</th>

                                                </tr>
                                            </thead>
                                            <tbody>
                                                @{ int i = 1; foreach (var item in Model)
                                                 {
                                                    <tr>
                                                        <td width="90%">@item.Name</td>
                                                        <td><a href="ProjectTypeMaster?Id=@item.Id"><i class="ft-edit-1"></i></a></td>
                                                    </tr>
                                                 }
                                                }
                                            </tbody>
                                        </table>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </section>
@Scripts.Render("~/bundles/dtjquery")
            <!--/ HTML5 export buttons table -->
<script>
    $(document).ready(function () {
        $("#btnGo").click(function () {
            window.location.href = 'ProjectTypeMaster';
        });
    });

</script>

         