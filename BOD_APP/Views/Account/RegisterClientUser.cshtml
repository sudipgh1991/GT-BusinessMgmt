
@{
    ViewBag.Title = "Register Client User";
    Layout = "~/Views/Shared/_RegisterClient.cshtml";
}
<form class="form-horizontal form-simple" method="post" novalidate="">
   
    
    <fieldset class="form-group floating-label-form-group">
        <label for="user-name">Enter Email</label>
        <input type="text" id="user-email" name="useremail" class="form-control" placeholder="Your Email Address">
    </fieldset>

   
    <fieldset class="form-group floating-label-form-group">
        <label for="user-name">Enter Password</label>
        <input type="password" id="user-passwordl" name="userpassword" class="form-control" placeholder="Enter Password" required="">
    </fieldset>

    <fieldset class="form-group floating-label-form-group">
        <label for="user-name">Enter Confirm Password</label>
        <input type="password" id="confirm-password" name="confirmpassword" class="form-control" placeholder="Enter Password" required="">
    </fieldset>


   
    <span class="btn btn-danger btn-block mb-5 err-msg" style="display:@ViewBag.UserId;">User Id already exist.</span>

    <span class="btn btn-danger btn-block mb-5 err-msg2" style="display:@ViewBag.Email;">Email already exist.</span>
    <span class="btn btn-danger btn-block mb-5 alert-msg" style="display:@ViewBag.GoingWrong;">Some thing is going wrong.</span>
    <button type="submit" class="btn btn-outline-info btn-block register">
        <i class="la la-spinner spinner btn-spinner-logo" style="display:none"></i>
        <i class="la la-user btn-user-logo"></i>
        Register
    </button>
</form>


